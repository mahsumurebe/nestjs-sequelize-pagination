{
  "name": "nestjs-sequelize-pagination",
  "version": "1.1.1",
  "description": "Pagination helper method for Sequelize models.",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint 'lib/**/*.ts' --fix",
    "prepublish": "npm run build",
    "publish": "npm publish --access public",
    "test:e2e": "jest --config ./tests/jest-e2e.json --runInBand",
    "test:e2e:dev": "jest --config ./tests/jest-e2e.json --runInBand --watch",
    "prerelease": "npm run build",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mahsumurebe/nestjs-sequelize-pagination.git"
  },
  "keywords": [
    "nestjs",
    "pagination",
    "sequelize"
  ],
  "author": "Mahsum UREBE <info@mahsumurebe.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mahsumurebe/nestjs-sequelize-pagination/issues"
  },
  "homepage": "https://github.com/mahsumurebe/nestjs-sequelize-pagination#readme",
  "devDependencies": {
    "@commitlint/cli": "17.6.5",
    "@commitlint/config-angular": "17.6.5",
    "@nestjs/common": "10.0.2",
    "@nestjs/core": "10.0.2",
    "@nestjs/platform-express": "10.0.2",
    "@nestjs/sequelize": "^10.0.0",
    "@nestjs/testing": "10.0.2",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.2",
    "@types/node": "18.16.18",
    "@types/supertest": "2.0.12",
    "@types/uuid": "9.0.2",
    "@typescript-eslint/eslint-plugin": "5.59.11",
    "@typescript-eslint/parser": "5.59.11",
    "eslint": "8.43.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "lint-staged": "13.2.2",
    "mysql": "2.18.1",
    "prettier": "2.8.8",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "sequelize": "6.32.1",
    "sequelize-typescript": "2.1.5",
    "supertest": "6.3.3",
    "ts-jest": "29.1.0",
    "typescript": "5.1.3"
  },
  "peerDependencies": {
    "@nestjs/common": "^8.0.0 || ^9.0.0 || ^10.0.0",
    "@nestjs/core": "^8.0.0 || ^9.0.0 || ^10.0.0",
    "@nestjs/sequelize": "^8.0.0 || ^9.0.0 || ^10.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "sequelize": "^6.3.5",
    "sequelize-typescript": "^2.0.0"
  },
  "lint-staged": {
    "**/*.{ts,json}": []
  },
  "dependencies": {
    "sqlite3": "^5.1.6"
  }
