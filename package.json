    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "lint-staged": "15.2.5",
    "mysql": "2.18.1",
    "prettier": "3.3.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "sequelize": "^6.37.3",
    "sequelize-typescript": "^2.1.6",
