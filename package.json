    "@typescript-eslint/eslint-plugin": "6.13.1",
    "@typescript-eslint/parser": "6.13.1",
    "eslint": "8.54.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.29.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "lint-staged": "15.1.0",
    "mysql": "2.18.1",
    "prettier": "3.1.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "sequelize": "6.35.1",
    "sequelize-typescript": "2.1.6",
